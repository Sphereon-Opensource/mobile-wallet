{
  "expo": {
    "name": "SphereonWallet",
    "slug": "SphereonWallet",
    "version": "0.1.2",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sphereon.ssi.wallet"
    },
    "android": {
      "package": "com.sphereon.ssi.wallet"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#202537"
    },
    "extra": {
      "eas": {
        "projectId": "76b1c6a4-d1d8-421e-8514-ee9db0b197b4"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SSI Mobile Wallet to use your location."
        }
      ]
    ]
  }
}
